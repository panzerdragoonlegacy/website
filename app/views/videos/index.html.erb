<% if @contributor_profile %>
  <% title "#{@contributor_profile.name}'s Videos" %>
<% elsif @tag %>
  <% title "Videos Tagged With #{@tag.name}" %>
<% else %>
  <% title 'Videos' %>
<% end %>

<% if @videos %>
  <%= render(
    partial: 'shared/videos',
    locals: { videos: @videos }
  ) %>
<% end %>
