<% if @contributor_profile %>
  <% title "#{@contributor_profile.name}'s Downloads" %>
<% elsif @tag %>
  <% title "Downloads Tagged With #{@tag.name}" %>
<% else %>
  <% title 'Downloads' %>
<% end %>

<% if @downloads %>
  <%= render(
    partial: 'shared/downloads',
    locals: { downloads: @downloads }
  ) %>
<% end %>
