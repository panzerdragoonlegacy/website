<% if @contributor_profile %>
  <% title "#{@contributor_profile.name}'s Quizzes" %>
<% elsif @tag %>
  <% title "Quizzes Tagged With #{@tag.name}" %>
<% else %>
  <% title 'Quizzes' %>
<% end %>

<% if @quizzes %>
  <%= render(
    partial: 'redesign/shared/quizzes',
    locals: { quizzes: @quizzes }
  ) %>
<% end %>
